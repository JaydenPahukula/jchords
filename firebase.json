{
  "hosting": [
    {
      "target": "main",
      "public": "./dist/",
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "editor",
      "public": "./dist-editor/",
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
